{
  "expo": {
    "name": "UPA",
    "slug": "THESIS",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainlogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/upaSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#953131"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby properties."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDuSnl17UeMperrQ26_EeKCJzrDMypKZVQ"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainlogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.paquiado20.THESIS",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDuSnl17UeMperrQ26_EeKCJzrDMypKZVQ"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app requires location access to show your position on the map."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "409ce64a-5179-4688-8cf4-f37ef275710b"
      }
    }
  }
}
